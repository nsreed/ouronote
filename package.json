{
  "name": "ouronote",
  "version": "0.0.3",
  "scripts": {
    "ng": "yarn",
    "start": "node app-server local --https-port 4321 --https-crt app-server/ouronote-dev.crt --https-key app-server/ouronote-dev.key --data-dir radata",
    "start:live": "ng serve --host 0.0.0.0 -c development",
    "start:live:prod": "ng serve -c production",
    "build": "ng build demo -c development",
    "build:all": "ng build log && ng build ng-gun && ng build demo -c development",
    "build:all:prod": "ng build log -c production && ng build ng-gun -c production && ng build demo -c production ",
    "build:watch": "ng build demo -c development --watch",
    "build:watch:prod": "ng build demo -c production --watch",
    "docker:build": "docker-compose build",
    "licenses": "license-checker --production --customPath licenseFormat.json --json > projects/demo/src/assets/licenses.json && node updateLicenses.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^8.0.0",
    "@angular/animations": "^14.0.6",
    "@angular/cdk": "^14.0.5",
    "@angular/common": "^14.0.6",
    "@angular/compiler": "^14.0.6",
    "@angular/core": "^14.0.6",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@angular/forms": "^14.0.6",
    "@angular/material": "^14.0.5",
    "@angular/platform-browser": "^14.0.6",
    "@angular/platform-browser-dynamic": "^14.0.6",
    "@angular/router": "^14.0.6",
    "@angular/service-worker": "^14.0.6",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@jsdevtools/host-environment": "^2.1.2",
    "@ngrx/store": "^15.4.0",
    "aspect-ts": "^1.0.6",
    "esbuild": "^0.14.49",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "gun": "nsreed/gun#ouronote",
    "hammerjs": "^2.0.8",
    "ngx-clipboard": "^15.1.0",
    "ngx-online-status": "^2.0.0",
    "nodemon": "^2.0.7",
    "paper": "^0.12.15",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.5.6",
    "safe-stable-stringify": "^2.3.1",
    "ts-aspect": "^3.0.4",
    "tslib": "^2.3.1",
    "util": "^0.12.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
    "@angular-devkit/build-angular": "^14.0.6",
    "@angular/cli": "^14.0.6",
    "@angular/compiler-cli": "^14.0.6",
    "@schematics/angular": "14.0.6",
    "@types/file-saver": "^2.0.5",
    "@types/gun": "^0.9.3",
    "@types/hammerjs": "^2.0.39",
    "@types/jasmine": "4.0.3",
    "@types/node": "18.0.6",
    "@types/qrcode": "^1.4.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sharedworker": "^0.0.76",
    "codelyzer": "^6.0.2",
    "folder-hash": "^4.0.2",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "license-checker": "^25.0.1",
    "ng-packagr": "^14.0.3",
    "protractor": "^7.0.0",
    "ts-node": "10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.7.4",
    "webpack": "^5.74.0",
    "yarn-audit-fix": "^9.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "radata/*",
      ".angular/*"
    ]
  },
  "engines": {
    "node": ">=14.x"
  }
}
